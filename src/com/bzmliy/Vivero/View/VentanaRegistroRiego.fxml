<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bzmliy.Vivero.View.VentanaRegistroRiegoController">
   <children>
      <ImageView fitHeight="427.0" fitWidth="570.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Image/29retopiensaverde1.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="tabla" layoutX="21.0" layoutY="114.0" prefHeight="185.0" prefWidth="333.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="40.0" text="id" />
          <TableColumn fx:id="nombre" prefWidth="170.0" text="nombre" />
            <TableColumn fx:id="tipo" prefWidth="122.0" text="tipo" />
        </columns>
      </TableView>
      <TextField fx:id="idProducto" alignment="CENTER" layoutX="21.0" layoutY="79.0" promptText="id producto" />
      <Button fx:id="ver" layoutX="170.0" layoutY="79.0" mnemonicParsing="false" onAction="#hacerClickVer" prefHeight="25.0" prefWidth="50.0" text="Ver" />
      <Button fx:id="actualizar" layoutX="220.0" layoutY="79.0" mnemonicParsing="false" onAction="#hacerClickActualizar" text="Actualizar" />
      <Button fx:id="eliminar" layoutX="288.0" layoutY="79.0" mnemonicParsing="false" onAction="#hacerClickEliminar" text="Eliminar" />
      <Button fx:id="agregar" layoutX="277.0" layoutY="322.0" mnemonicParsing="false" onAction="#hacerClickAgregar" text="Agregar" />
      <Button fx:id="back" layoutX="21.0" layoutY="322.0" mnemonicParsing="false" onAction="#hacerClickMenu" text="&lt;-     |     Regresar">
         <font>
            <Font name="BLACK OPS 2" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
