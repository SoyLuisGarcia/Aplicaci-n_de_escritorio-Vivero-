<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bzmliy.Vivero.View.VideoController">
   <children>
      <VBox fx:id="video" alignment="CENTER" prefHeight="600.0" prefWidth="600.0">
         <children>
            <MediaView fx:id="videazo" fitHeight="400.0" fitWidth="500.0" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#reproducirClick" text="Reproducir" />
                  <Button mnemonicParsing="false" onAction="#pararClick" text="Parar" />
                  <Button mnemonicParsing="false" onAction="#salirClick" text="Salir" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
